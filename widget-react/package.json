{
  "name": "@fastn-ai/widget-react",
  "version": "1.0.0",
  "description": "React version of the Fastn widget",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "fastn-widget",
    "react",
    "fastn",
    "widget"
  ],
  "author": "fastn.ai",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.2.2"
  }
}
